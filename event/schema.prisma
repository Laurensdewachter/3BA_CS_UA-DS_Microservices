datasource db {
  provider = "postgresql"
  url      = "postgresql://root:glowing-banana@event-db:5432/event_db"
}

generator db {
  provider             = "prisma-client-py"
  interface            = "asyncio"
  recursive_type_depth = 5
}

model event {
  title     String   @id
  organizer String
  date      DateTime
  public    Boolean
}
