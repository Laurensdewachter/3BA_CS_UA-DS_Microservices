datasource db {
  provider = "postgresql"
  url      = "postgresql://root:glowing-banana@user-db:5432/user_db"
}

generator db {
  provider             = "prisma-client-py"
  interface            = "asyncio"
  recursive_type_depth = 5
}

model User {
  id       String @id @default(cuid())
  username String @unique
  password String
}
